{
  "name": "healthy-plan-api",
  "version": "1.0.0",
  "description": "Healthy Plan API",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsup src --out-dir build",
    "start": "node build/server.js",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:dev": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test:unit": "vitest run -c vite.config.ts",
    "test:watch": "vitest -c vite.config.ts",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run -c vitest.config.ts",
    "test:e2e:watch": "vitest -c vitest.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "sonar:scanner": "sonar-scanner",
    "lint": "eslint src --fix",
    "test:load": "artillery run -o load_tests/results.json load_tests/artillary-conf.yml",
    "test:load:view": "artillery report -o load_tests/results.html load_tests/results.json"
  },
  "keywords": [],
  "author": "Lucas Lima",
  "license": "ISC",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/node": "^20.4.6",
    "@types/pdfmake": "^0.2.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "@vitest/coverage-v8": "^0.34.1",
    "@vitest/ui": "^0.34.1",
    "artillery": "^2.0.0-36",
    "eslint": "^8.46.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.1.0",
    "sonarqube-scanner": "^3.0.1",
    "supertest": "^6.3.3",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "@fastify/cookie": "^9.0.4",
    "@fastify/cors": "^8.3.0",
    "@fastify/jwt": "^7.2.0",
    "@fastify/swagger": "^8.9.0",
    "@fastify/swagger-ui": "^1.9.3",
    "@prisma/client": "^5.1.0",
    "@types/swagger-ui": "^3.52.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "excel4node": "1.8.2",
    "fastify": "^4.21.0",
    "pdfmake": "^0.2.7",
    "redis": "^4.6.8",
    "zod": "^3.21.4"
  }
}
